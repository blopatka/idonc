<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="org.lopatka.idonc.web.model.data" default-access="field">
	<class name="IdoncProject" table="PROJECTS">
		<cache usage="read-write"/>
		<id name="id" column="ID">
			<generator class="native"/>
		</id>
		
		<property name="name" type="string" length="255" column="NAME" not-null="true"/>
		<property name="website" type="string" length="255" column="WEBSITE"/>
		<property name="description" type="string" length="255" column="DESCRIPTION"/>
		
		
	</class>
</hibernate-mapping>
